# Environment
VITE_ENV=development
VITE_APP_NAME=E-Commerce Seller Portal

# API Configuration
VITE_AUTH_API=http://localhost:3010
VITE_PRODUCT_API=http://localhost:3011
VITE_ORDER_API=http://localhost:3012
VITE_CATEGORY_API=http://localhost:3013

# Cloudinary Configuration (Image Upload)
VITE_CLOUDINARY_CLOUD_NAME=your-cloud-name
VITE_CLOUDINARY_UPLOAD_PRESET=your-upload-preset

# Feature Flags
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_DEBUG_MODE=true

# API Configuration
VITE_API_TIMEOUT=30000

# Error Tracking (Sentry)
VITE_SENTRY_DSN=

# Shell App URL (for micro-frontend communication)
VITE_SHELL_APP_URL=http://localhost:3000
